<% content_for :title do %> Display All Users <% end %>
<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @test %>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %> <%= user.last_name %></td>
        <td><%= user.email_address %></td>
      </tr>
    <% end %>
  </tbody>
</table>
